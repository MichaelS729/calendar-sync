$('#panel').html('#{j render 'default_panel', locals: {msg: @msg}}');

- if @success
  var event = {id : #{@event.id},title : "#{@event.name}", start : "#{@event.start.iso8601}", stop : "#{(stop = @event.end) ? stop.iso8601 : nil}", third_party : #{Meetup.new.default_group_name != @event.organization}, url : "#{Rails.application.routes.url_helpers.event_path(@event.id)}"};
  $('#calendar').fullCalendar('renderEvent', event, true);